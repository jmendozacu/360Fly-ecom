<config>
    <sections>
        <carriers translate="label" module="shipping">
            <groups>
                <shipper translate="label">
                    <fields>
                        <enable_lookup translate="label">
                            <label>Enable Suburb Lookup</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>395</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </enable_lookup>
                        <max_results translate="label">
                            <label>Maximum Number of Results</label>
                            <frontend_type>text</frontend_type>
                            <validate>validate-number</validate>
                            <depends><enable_lookup>1</enable_lookup></depends>
                            <sort_order>396</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </max_results>
                    </fields>
                </shipper>
            </groups>
        </carriers>
    </sections>
</config>
